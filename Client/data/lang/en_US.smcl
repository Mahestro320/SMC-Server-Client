SECTION command_info;
	SECTION help;
		.short_description = "show help";
		.long_description = "usage: help <command name (optional)>\n"
		                    "if command name is given, it prints the long description of it\n"
		                    "wait... you already know that because you had to do that to access this text :)";
	END;

	SECTION infos;
		.short_description = "print shell infos";
	END;

	SECTION clear;
		.short_description = "clear the screen";
	END;

	SECTION quit;
		.short_description = "exit the shell";
	END;

	SECTION conf;
		.long_description = "usage: conf <option> [option args]\n"
		                    "the options are:\n"
		                    "\tload    load file\n"
		                    "\tsave    update file with new values\n"
		                    "\tprint   prints all the values\n"
		                    "\tset     set a value with a key using [<key> <value>]\n"
		                    "\tget     gets a value from a key using [<key>]";
	END;

	SECTION print;
		.short_description = "prints text";
		.long_description = "usage: print <text...>";
	END;

	SECTION login;
		.short_description = "log in as an user";
		.long_description = "usage: login <username> <password>";
	END;

	SECTION logout;
		.short_description = "log out";
	END;

	SECTION client;
		.long_description = "usage: client <option>\n"
		                    "the options are:\n"
                            "\tconnect      connects to the server\n"
                            "\tdisconnect   disconnects from the server";
	END;

	SECTION server;
		.long_description = "usage: server <option <sub option>>\n"
		                    "the options are:\n"
                            "\tget-version   print the server version\n"
                            "\trestart (admin):\n"
                            "\t\tprogram   (default) restart the program\n"
                            "\t\tmachine   reboot the machine";
	END;

	SECTION dir;
		.short_description = "show the content of the current directory";
		.long_description = "usage: dir <folder (optional)>\n";
	END;

	SECTION chdir;
		.short_description = "sets the current directory";
		.long_description = "usage: chdir <location>\n"
		                    "the location is relative to the current directory";
	END;

	SECTION download;
		.short_description = "download a file";
		.long_description = "usage: download <input path>";
	END;

	SECTION upload;
		.short_description = "upload a file";
		.long_description = "usage: upload <input path (client)> <output path (optional)>";
	END;

	SECTION sysmcd;
		.short_description = "executes a server system command";
		.long_description = "usage: syscmd <command>";
	END;

	SECTION mkfile;
		.short_description = "creates a file";
		.long_description = "usage: mkfile <filename>";
	END;

	SECTION mkdir;
		.short_description = "creates a directory";
		.long_description = "usage: mkdir <filename>";
	END;

	SECTION delete;
		.short_description = "delete a file / directory";
		.long_description = "usage: delete <filepath>";
	END;

	SECTION rename;
		.short_description = "rename a file / directory";
		.long_description = "usage: rename <filepath> <new name>";
	END;

	SECTION edit;
		.short_description = "modify a text file";
		.long_description = "usage: edit <filepath>";
	END;

	SECTION lang;
		.short_description = "manage shell language";
		.long_description = "usage: lang <options...>\n"
		                    "the options are:\n"
							"\tinfo         show current language infos\n"
							"\tset          set the current language\n"
							"\t\t<language name / file name>\n"
							"\treload       reload the current language";
	END;

	SECTION user;
		.short_description = "manage users: add, remove, ect.";
		.long_description = "usage: user <options...>\n"
		                    "the options are:\n"
							"\tadd <new username>        add a new user\n"
							"\tdel <target username>     delete a specified user\n"
							"\tban <target username>     ban a specified user\n"
							"\tunban <target username>   unban a specified user\n"
							"\tbanip <target ip>         ban an ip\n";
	END;

	SECTION move;
		.short_description = "move a file";
		.long_description = "usage: move <input path> <output path>\n";
	END;

	SECTION copy;
		.short_description = "copy a file";
		.long_description = "usage: copy <input path> <output path>\n";
	END;
END;

